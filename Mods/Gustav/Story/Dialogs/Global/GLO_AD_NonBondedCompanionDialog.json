      "TaggedTextList": [
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "ffd08582-7396-4cac-bcd4-8f9cd0fd8ef3"
                ],
                "TagNames": [
                  "REALLY_ASTARION"
                ]
              }
            ],
            "RuleString": "REALLY_ASTARION"
          },
          "TagTexts": [
            {
              "LineId": "b7f5c31c-10cb-af35-7d24-708d8931698b",
              "Text": {
                "Handle": "h7e1d6ad7gd641g4863gaa20g62d2f93d61db",
                "Value": "I have a travelling companion. I hardly need another."
              }
            },
            {
              "LineId": "6833899c-8e7e-a25c-b0e3-a49e3bb6e044",
              "Text": {
                "Handle": "hfbd90b7cg4af1g4121g8feagc8ed8c61159b",
                "Value": "I'm not interested in talking to you."
              }
            },
            {
              "LineId": "29d3fed7-180c-0310-91b3-92dc8e300fed",
              "Text": {
                "Handle": "h00da4993gaeb1g482cgbce9g43347b416b5d",
                "Value": "Do you mind? I'm <i>very </i>busy."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "9b0354c0-56d9-4723-8034-918ac9abab19"
                ],
                "TagNames": [
                  "REALLY_GALE"
                ]
              }
            ],
            "RuleString": "REALLY_GALE"
          },
          "TagTexts": [
            {
              "LineId": "dc732fd4-2227-6789-84b0-468acdbb0155",
              "Text": {
                "Handle": "hd0012838g8fd8g4c5cg8f8egfb5fe9e89b73",
                "Value": "Sorry, but I'm spoken for."
              }
            },
            {
              "LineId": "92ee7596-b61e-ee05-55b1-e2b9876d66ce",
              "Text": {
                "Handle": "h169ac011g1345g413bga592g3f6492c413f7",
                "Value": "Do excuse me, please. Frightfully busy."
              }
            },
            {
              "LineId": "c20d1d1b-db13-d3db-8bb0-0fac27753979",
              "Text": {
                "Handle": "h1d91ae1eg29d9g4ee2gb333ge4342ed638b8",
                "Value": "Whatever it is, I'm afraid I can't help you."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "1a2f70d6-8ead-4eb5-a824-79ee1971764a"
                ],
                "TagNames": [
                  "REALLY_KARLACH"
                ]
              }
            ],
            "RuleString": "REALLY_KARLACH"
          },
          "TagTexts": [
            {
              "LineId": "07679d9b-be45-affa-43e5-d937bcaaa532",
              "Text": {
                "Handle": "h0e4247eeg18f6g40e2g809fga232abbcf7e6",
                "Value": "Hey. Bye."
              }
            },
            {
              "LineId": "1fcdd1ff-ef9c-4bc5-9014-ec83c4fae945",
              "Text": {
                "Handle": "h80ecc367g79f6g40b5gb4d2g0b215143975d",
                "Value": "Sorry, I've already got a best friend."
              }
            },
            {
              "LineId": "4d1f3f2d-772a-4c1b-8dcc-2d0bdc185fc2",
              "Text": {
                "Handle": "h15b98ec1gbaf3g419cgba2dg70ca59c1a61f",
                "Value": "Sorry, I don't have any change."
              }
            },
            {
              "LineId": "84ccc764-8bfa-4ba6-bcfa-2ed745878b9e",
              "Text": {
                "Handle": "h3fbeef70gb680g41e1gb316g0b64eed4170d",
                "Value": "I'll have to admire you from afar."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "b5682d1d-c395-489c-9675-1f9b0c328ea5"
                ],
                "TagNames": [
                  "REALLY_LAEZEL"
                ]
              }
            ],
            "RuleString": "REALLY_LAEZEL"
          },
          "TagTexts": [
            {
              "LineId": "ba3aa29a-1858-5978-331d-25c33d5ce966",
              "Text": {
                "Handle": "ha3ade107gbe07g4047gbe5bgc20bf13b5b4f",
                "Value": "I have allied with another."
              }
            },
            {
              "LineId": "18f41ec5-ba43-3eb0-a742-659d55bd7751",
              "Text": {
                "Handle": "hb95b300fg0c95g4c10g9623g31ed379b8947",
                "Value": "I've no business with you."
              }
            },
            {
              "LineId": "cfaab5a0-3c0b-1eb0-bf5e-3eadd14ca166",
              "Text": {
                "Handle": "ha2b1499age533g4014gb851g4ec3050d00ec",
                "Value": "<i>Chk. </i>I've no need for friends."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "642d2aee-e3df-47e3-9f47-bbcd441bb9e0"
                ],
                "TagNames": [
                  "REALLY_SHADOWHEART"
                ]
              }
            ],
            "RuleString": "REALLY_SHADOWHEART"
          },
          "TagTexts": [
            {
              "LineId": "4642e9f7-4beb-aa89-73c6-3b816b977ade",
              "Text": {
                "Handle": "he298665fgc438g429bg86f4g14a3bad2e6ce",
                "Value": "There's nothing for you and I to discuss. "
              }
            },
            {
              "LineId": "78cbd8fa-9361-e918-35d6-648b6eb2ff3c",
              "Text": {
                "Handle": "h37a6bb0cgd46ag4565g9f4dg4181c7b9ca4d",
                "Value": "You'll have to confide in someone else. I'm spoken for."
              }
            },
            {
              "LineId": "afc80a5f-9515-3127-78dd-793b0e05caa5",
              "Text": {
                "Handle": "h984b1965g457cg48c9g9ab9g413ad4e0eb02",
                "Value": "You'll have to look elsewhere for company - I already have an arrangement. "
              }
            },
            {
              "LineId": "28c1061e-b9e5-bd09-cebe-98df3f99b72d",
              "Text": {
                "Handle": "h12d8e22ag93a8g4cd9ga822gc224fd88c90b",
                "Value": "There's little I can say to you, I'm afraid. "
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "5f40def5-d3ec-4698-a367-01a339888956"
                ],
                "TagNames": [
                  "REALLY_WYLL"
                ]
              }
            ],
            "RuleString": "REALLY_WYLL"
          },
          "TagTexts": [
            {
              "LineId": "341c013a-571e-2008-f93c-69835d1ac20f",
              "Text": {
                "Handle": "hcd40eef3g9669g4bdcgb266g217089c53c8b",
                "Value": "Sorry - there's only so much of me to go around."
              }
            },
            {
              "LineId": "36e95832-8889-1b1e-18f8-4d5209b5b40c",
              "Text": {
                "Handle": "h1fd57197ge8adg47c3gb312g897294fb7c3e",
                "Value": "You've got gusto, but I'm bound to another."
              }
            },
            {
              "LineId": "1e8683cf-0555-edfc-b9a8-015dd6cc8a09",
              "Text": {
                "Handle": "h7f90ea7ag9963g4166ga12eg5caec91b01a0",
                "Value": "You seem nice and all, but I've teamed up with someone else."
              }
            }
          ]
        }
