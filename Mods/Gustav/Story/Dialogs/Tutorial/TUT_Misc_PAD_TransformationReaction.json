            "RuleString": "REALLY_GALE"
          },
          "TagTexts": [
            {
              "LineId": "17e9dab1-6b99-53a1-d2e2-ae30c8a63b4e",
              "Text": {
                "Handle": "h8e4c9b36gc814g47dcgbcf5g863a0ce2f3b4",
                "Value": "Gods, it's horrifying... and a touch fascinating."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
