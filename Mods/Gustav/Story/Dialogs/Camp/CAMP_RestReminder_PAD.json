      "TaggedTextList": [
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "ffd08582-7396-4cac-bcd4-8f9cd0fd8ef3"
                ],
                "TagNames": [
                  "REALLY_ASTARION"
                ]
              }
            ],
            "RuleString": "REALLY_ASTARION"
          },
          "TagTexts": [
            {
              "LineId": "50b04e74-f483-9830-e9fc-4bcb21f42b84",
              "Text": {
                "Handle": "h16c7bc79gead9g47e9gaf00g123598cc9beb",
                "Value": "I need to rest. Sunset can't come quickly enough."
              }
            },
            {
              "LineId": "3a0e27a4-559c-0027-09f2-6bbf2e269eae",
              "Text": {
                "Handle": "h8f632e39g7130g4fd5gaf6bgf4be72e24d27",
                "Value": "Ugh. There had better be a soft bedroll in my near future."
              }
            },
            {
              "LineId": "a8582264-edea-f807-1c38-be8496408779",
              "Text": {
                "Handle": "h2a2a2c1fg30aag4fb7g8e26gf91fca7ceee0",
                "Value": "I'm so tired. How do people do this all day?"
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "9b0354c0-56d9-4723-8034-918ac9abab19"
                ],
                "TagNames": [
                  "REALLY_GALE"
                ]
              }
            ],
            "RuleString": "REALLY_GALE"
          },
          "TagTexts": [
            {
              "LineId": "1bea0b65-f660-e6db-8608-98795b1b55bc",
              "Text": {
                "Handle": "h37528696g45d1g4951g83a4g561f3b0ba4f0",
                "Value": "I'm quite exhausted. A good night's sleep would do me wonders."
              }
            },
            {
              "LineId": "3213ba87-5b99-178b-e1c8-c48589d7a2f3",
              "Text": {
                "Handle": "h3147d30bg6ea4g4861gbaa4g4758a3b672e1",
                "Value": "If I don't get my beauty sleep soon, I may just get a tad malcontent."
              }
            },
            {
              "LineId": "d83fadbf-8fdc-655b-db16-ee53364f9598",
              "Text": {
                "Handle": "hedab0b63g23e3g4ddbg9919gb73da1a99fa6",
                "Value": "A wizard's quite worthless with a tired mind. Sleep beckons, and I would follow."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "1a2f70d6-8ead-4eb5-a824-79ee1971764a"
                ],
                "TagNames": [
                  "REALLY_KARLACH"
                ]
              }
            ],
            "RuleString": "REALLY_KARLACH"
          },
          "TagTexts": [
            {
              "LineId": "1f4e3dbb-c738-5a5c-17ea-c770532ee008",
              "Text": {
                "Handle": "hd4e94a00ga7a6g4983g8b29g75da287064c6",
                "Value": "My dogs are barking. Can we rest?"
              }
            },
            {
              "LineId": "4f8b8b56-ab63-46cb-aa28-ac89bb681b6e",
              "Text": {
                "Handle": "h4e1bc71egb326g438cg8587g7168995ee339",
                "Value": "*Yawn!*"
              }
            },
            {
              "LineId": "a6810c69-99de-4d17-97a1-3e9fc44bd49f",
              "Text": {
                "Handle": "h9e5e8f8eg40bfg4ee7gad63g17dc128e6104",
                "Value": "Time to take a load off, isn't it?"
              }
            },
            {
              "LineId": "5025f7fd-cd22-4a1c-83d1-d23d8ac08a94",
              "Text": {
                "Handle": "h4f1f6809gb799g4133g8eabge6e517eb35fc",
                "Value": "I am ready to rest my weary head."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "b5682d1d-c395-489c-9675-1f9b0c328ea5"
                ],
                "TagNames": [
                  "REALLY_LAEZEL"
                ]
              }
            ],
            "RuleString": "REALLY_LAEZEL"
          },
          "TagTexts": [
            {
              "LineId": "e9a029bf-dd2d-9984-1599-594e35347adf",
              "Text": {
                "Handle": "hc682654dgcd70g431fgb412g72982a9e9262",
                "Value": "Battle has taken its toll. I should rest."
              }
            },
            {
              "LineId": "1ef8cb52-c00d-4cf9-a1ec-e701eb34e704",
              "Text": {
                "Handle": "h2968194bg6f38g44b0gaafdgac7551209392",
                "Value": "My energy wanes - time to rest."
              }
            },
            {
              "LineId": "cc9e263b-9f04-488e-af1b-38c9b56c9f03",
              "Text": {
                "Handle": "h55b52989g9951g46f0gbe14g7a879e3f8fb3",
                "Value": "My eyes grow heavy. Best to make camp."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "642d2aee-e3df-47e3-9f47-bbcd441bb9e0"
                ],
                "TagNames": [
                  "REALLY_SHADOWHEART"
                ]
              }
            ],
            "RuleString": "REALLY_SHADOWHEART"
          },
          "TagTexts": [
            {
              "LineId": "cf0af4fa-5e3a-4ea0-dd37-ecfa4427d6a0",
              "Text": {
                "Handle": "h6054f512gf280g4b0eg9ba9gd0abd3c0bc4c",
                "Value": "It's been a long day. Better stop to rest soon."
              }
            },
            {
              "LineId": "ca2eb0af-d819-48cb-83bb-96a7c00c96f3",
              "Text": {
                "Handle": "h67d796a1g16acg4d89g9f8cg7f0e359788f5",
                "Value": "Better make camp soon. Might be a while before there's another chance to rest."
              }
            },
            {
              "LineId": "a23f5303-a94b-4ffc-980d-71fb1ddf1060",
              "Text": {
                "Handle": "h0e757328gd723g4a80g93d8g35a7a466bf8a",
                "Value": "Should get some rest - I need to keep sharp."
              }
            },
            {
              "LineId": "bd70bbd5-ce2c-4408-8dae-718ff8076825",
              "Text": {
                "Handle": "hd94f043bg92abg45b8g9088g761bad0c6e84",
                "Value": "Time to pack it in for the day, I think. "
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "5f40def5-d3ec-4698-a367-01a339888956"
                ],
                "TagNames": [
                  "REALLY_WYLL"
                ]
              }
            ],
            "RuleString": "REALLY_WYLL"
          },
          "TagTexts": [
            {
              "LineId": "930bf4a2-553a-fe0f-5b76-a6ec0aa07de2",
              "Text": {
                "Handle": "hbb1bb8c0gb3d3g4157g9273g9d7826d6a826",
                "Value": "What a day. I could really use some shut-eye."
              }
            },
            {
              "LineId": "29c014e0-f8b2-469b-9969-edbe43339273",
              "Text": {
                "Handle": "heb8ac3eaga1d3g497cg8136gbace7f632f24",
                "Value": "Whew - I'm spent. I should unwind in camp."
              }
            },
            {
              "LineId": "0933a3c0-00e7-40b0-a313-2a17abdee270",
              "Text": {
                "Handle": "h0dc6048ag9a81g4cc3g8ffeg27ecc28fb8ab",
                "Value": "What I wouldn't give for a good rest about now."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "264a6880-9a51-429c-a9fc-97f8952baf90"
                ],
                "TagNames": [
                  "REALLY_GENERIC"
                ]
              }
            ],
            "RuleString": "REALLY_GENERIC"
          },
          "TagTexts": [
            {
              "LineId": "03d01b50-fe65-0476-2c89-286bbcc698b1",
              "Text": {
                "Handle": "h37ff1804ged60g4277g8822g40f81a493305",
                "Value": "I'm exhausted. Better find somewhere to camp soon."
              }
            },
            {
              "LineId": "efacae2a-818c-e189-8f43-abbfb71a5896",
              "Text": {
                "Handle": "hfcd4c632gdc6dg44adg81e1g745777967040",
                "Value": "It's been a long day. I'll need to rest soon."
              }
            },
            {
              "LineId": "e87a33c4-9df5-069a-6e8c-1852cc5af6c3",
              "Text": {
                "Handle": "h75d34badg6d8dg4099g90bcg42357b3696ea",
                "Value": "I'm done. I can't wait to sleep."
              }
            }
          ]
        }
