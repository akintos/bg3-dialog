            },
            {
              "LineId": "6de3eb9a-eb5d-4980-d5a0-30936ded1e8f",
              "Text": {
                "Handle": "h10c5f024gbabfg463egb614g8076e603f14f",
                "Value": "Be careful!"
              }
            },
            {
              "LineId": "433ff6b4-3ecc-a6dc-a51a-7c7130463e46",
              "Text": {
                "Handle": "hdd92e750gf99eg4d3aga7ecgedbc690073f9",
                "Value": "You damned idiot."
              }
            },
            {
              "LineId": "c1b888a0-f099-3a9c-50b3-e5092d2edf07",
              "Text": {
                "Handle": "hacb6ce17g64f2g4730ga367g0ee6061dd31c",
                "Value": "Stop it!"
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
