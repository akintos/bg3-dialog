            {
              "LineId": "bb4b6788-218d-4449-a595-2a8a2b3a7c8c",
              "Text": {
                "Handle": "hed62e73eg4761g49b3gabd1g1f92837226a8",
                "Value": "We will follow, Boo - but not until the sunrise has the vampires snoozing in their upside-down beds."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "3e84e1cd-2193-4f9f-80b4-c2ededefaea6"
                ],
                "TagNames": [
                  "REALLY_MINTHARA"
                ]
              }
            ],
            "RuleString": "REALLY_MINTHARA"
          },
          "TagTexts": [
            {
              "LineId": "8fbede04-123b-4b49-8ea3-1bb65e7baa5f",
              "Text": {
                "Handle": "hf9eb53adg3ee8g4d0egb821g34aa3fc326a8",
                "Value": "Vampires thrive in the darkness even more than I do. Best wait 'til morning before we strike."
              }
            }
          ]
        },
