          "TagTexts": [
            {
              "LineId": "91dff2c5-0533-4697-9d7a-f92531e2c859",
              "Text": {
                "Handle": "h579b5fa0g6573g4b3ag95dcg2bb8c00938f0",
                "Value": "I'm feeling rather unsteady..."
              }
            },
            {
              "LineId": "4083cb20-ad6d-41c8-9287-18a93ba4041a",
              "Text": {
                "Handle": "h0a166f86gcaedg45ffgb803gf17055048f57",
                "Value": "Deep breaths, Gale. Hold it together."
              }
            },
            {
              "LineId": "c49016dd-e908-4b7a-bd97-b273934d8488",
              "Text": {
                "Handle": "ha9e390f5g258bg483eg9586geef44a2887ea",
                "Value": "I'd be rather enjoying this journey, were I not wracked with pain."
              }
            },
            {
              "LineId": "a7459c74-4068-456b-9a3c-350a8fa1af37",
              "Text": {
                "Handle": "h1d1eb577g588cg4b1fgb519g5e939221deff",
                "Value": "One step in front of the other, Gale."
              }
            },
            {
              "LineId": "2ec5db67-9c75-4448-8be7-565b57d77407",
              "Text": {
                "Handle": "h21d4f27cgc997g4618g86fcgdc47d867b922",
                "Value": "Ugh, I feel rotten."
              }
            },
            {
              "LineId": "8d31cdf0-0060-4056-b47d-7b9ada88e764",
              "Text": {
                "Handle": "h3e471e50g8b6eg4ccfgb54bg7e0aad248d90",
                "Value": "I don't feel right. Not even close."
              }
            },
            {
              "LineId": "200b314f-97f8-4b3f-8f0f-33627622e287",
              "Text": {
                "Handle": "h27dfa68fgc753g4cfbg97beg3f5fa5edde81",
                "Value": "Getting weaker by the moment."
              }
            },
            {
              "LineId": "e63b1e85-0a8f-4748-85b3-d2b8baedf7c8",
              "Text": {
                "Handle": "h4713c83age8f9g4c56ga125ga45ea4d5685f",
                "Value": "Feeling more unwell than ever."
              }
            },
            {
              "LineId": "6d142016-1e07-4f5f-a71d-17dcdfb3ab8e",
              "Text": {
                "Handle": "h1251843eg39aeg481dga221ge675102c38ae",
                "Value": "What I wouldn't give for some respite."
              }
            },
            {
              "LineId": "889121b5-0f9a-413a-bc26-2c0ca06b5e50",
              "Text": {
                "Handle": "hf9d65092g5453g497cg8638g1cfb5b093ea3",
                "Value": "Gods, this is bloody uncomfortable."
              }
            },
            {
              "LineId": "287bb49d-1c31-4da6-a5dc-5af7cc422adc",
              "Text": {
                "Handle": "h3ab46ca3gce96g4cfbg9fc0g7fbe1d54c60a",
                "Value": "I hope I don't deteriorate further."
              }
            },
            {
              "LineId": "2068cb73-2c90-4e77-b3a8-2eb1e27be63a",
              "Text": {
                "Handle": "h5b10447cg62a0g490cgb458gc8fc657bb71f",
                "Value": "Struggling more with every moment."
              }
            },
            {
              "LineId": "db369559-3a19-4a35-9c0b-9db081e5e845",
              "Text": {
                "Handle": "h8911c3c0g1122g41f6gb361g65cfef0cbf9f",
                "Value": "My condition's only getting worse."
              }
            }
          ]
        }
