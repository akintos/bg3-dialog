      "TaggedTextList": [
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "9b0354c0-56d9-4723-8034-918ac9abab19"
                ],
                "TagNames": [
                  "REALLY_GALE"
                ]
              }
            ],
            "RuleString": "REALLY_GALE"
          },
          "TagTexts": [
            {
              "LineId": "81a8f5e6-82a7-48b3-a822-2a84c296eafb",
              "Text": {
                "Handle": "h7e2c0932g2266g4372gb265g834a969fff57",
                "Value": "Marvellous. But why hide such splendour away where none can admire it?"
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "1a2f70d6-8ead-4eb5-a824-79ee1971764a"
                ],
                "TagNames": [
                  "REALLY_KARLACH"
                ]
              }
            ],
            "RuleString": "REALLY_KARLACH"
          },
          "TagTexts": [
            {
              "LineId": "c68dfa4e-2f0f-447e-ba21-9f3ed2ab749f",
              "Text": {
                "Handle": "hc72c2e87gab40g4f94g93c8g1acf3a3f5225",
                "Value": "Would you look at this. Here all this time, and I never knew."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "b5682d1d-c395-489c-9675-1f9b0c328ea5"
                ],
                "TagNames": [
                  "REALLY_LAEZEL"
                ]
              }
            ],
            "RuleString": "REALLY_LAEZEL"
          },
          "TagTexts": [
            {
              "LineId": "c713383f-c212-4a31-a067-a61a81ef121f",
              "Text": {
                "Handle": "h815ab622g6defg4eceg83a2ga758aaaf22fc",
                "Value": "A striking view. I could never deny it."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "aeb694fc-83fb-452d-819a-b97ba442dc42"
                ],
                "TagNames": [
                  "REALLY_MINSC"
                ]
              }
            ],
            "RuleString": "REALLY_MINSC"
          },
          "TagTexts": [
            {
              "LineId": "d40b9c1d-fb6d-4f04-afd7-c32770102100",
              "Text": {
                "Handle": "hd05b3d52g589eg4400ga5f7g3e68cf47ad4e",
                "Value": "Look, Boo! Such cavernous ruins - this would have made you a fine lair."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "642d2aee-e3df-47e3-9f47-bbcd441bb9e0"
                ],
                "TagNames": [
                  "REALLY_SHADOWHEART"
                ]
              }
            ],
            "RuleString": "REALLY_SHADOWHEART"
          },
          "TagTexts": [
            {
              "LineId": "dd2a05e7-0a4e-4d99-b94b-82fa14c027d3",
              "Text": {
                "Handle": "hf7c9ca85g771ag4f44gaca4g5ae1589c3641",
                "Value": "Quite the work of art. A shame to have it hidden away."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "8457eb5f-036c-4143-b6cf-447a9f555c7a"
                ],
                "TagNames": [
                  "REALLY_JAHEIRA"
                ]
              }
            ],
            "RuleString": "REALLY_JAHEIRA"
          },
          "TagTexts": [
            {
              "LineId": "374d5ee1-2849-431c-976f-3c9c0a409555",
              "Text": {
                "Handle": "h88078c41gd1d2g47faga825g66575169b4cf",
                "Value": "Huh. It stands to reason the prettiest part oc the city is buried beneath it."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "3e84e1cd-2193-4f9f-80b4-c2ededefaea6"
                ],
                "TagNames": [
                  "REALLY_MINTHARA"
                ]
              }
            ],
            "RuleString": "REALLY_MINTHARA"
          },
          "TagTexts": [
            {
              "LineId": "3fce8c3e-0dab-40e3-a80d-ac29745273cb",
              "Text": {
                "Handle": "hb7f6972fg0087g4513gaa54g86c90d5feed4",
                "Value": "There is beauty to be found in Baldur's Gate after all. Little wonder that it lies beneath the streets."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "9b8709f9-8d2a-4b4e-a465-8505c561d7f5"
                ],
                "TagNames": [
                  "REALLY_HALSIN"
                ]
              }
            ],
            "RuleString": "REALLY_HALSIN"
          },
          "TagTexts": [
            {
              "LineId": "0097ddfc-fc52-46b4-9486-23e259d7b2a0",
              "Text": {
                "Handle": "h1cff1e01gda9eg4abbgafceg800339a3dbf6",
                "Value": "Astonishing. This city can be ugly at times, but it can be capable of grandeur as well..."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "5f40def5-d3ec-4698-a367-01a339888956"
                ],
                "TagNames": [
                  "REALLY_WYLL"
                ]
              }
            ],
            "RuleString": "REALLY_WYLL"
          },
          "TagTexts": [
            {
              "LineId": "3e94ad04-023a-4618-b9a3-fa98982f1177",
              "Text": {
                "Handle": "h39e471c1gfbb6g41cbgb8dbg914f418934b2",
                "Value": "Astonishing. To think such grandeur has been lurking beneath the city."
              }
            }
          ]
        },
        {
          "HasTagRule": false,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [],
            "RuleString": ""
          },
          "TagTexts": [
            {
              "LineId": "4903965f-8a2d-44eb-9b74-f30436a1ad93",
              "Text": {
                "Handle": "h38b274a2g0dd0g425dgb977ge8a3dfc6cbc6",
                "Value": "Incredible. How old <i>is</i> this place?"
              }
            }
          ]
        }
