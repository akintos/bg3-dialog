            "RuleString": "REALLY_JAHEIRA"
          },
          "TagTexts": [
            {
              "LineId": "7c0198b6-8210-4463-a0e5-766402b38fc9",
              "Text": {
                "Handle": "ha2bfdf15gb944g45cega3b6g0beb26834fd5",
                "Value": "Neverwinter. I had to break my cover there to stop a killing - the goat-form was <i>not</i> kind to my dress."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
