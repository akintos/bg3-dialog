          "TagTexts": [
            {
              "LineId": "5bf8a205-c4d6-48bc-ab85-fe8f7e7f9020",
              "Text": {
                "Handle": "h9ea4ee77g0447g4253g8f98gc3a341a17bf7",
                "Value": "Neverwinter, well to the north of here. The slates in our library at K'liir made mention of it."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
