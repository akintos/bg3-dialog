            "RuleString": "REALLY_GALE"
          },
          "TagTexts": [
            {
              "LineId": "f46bc2f3-876e-4e15-9fa5-e2ddb6ba6bb6",
              "Text": {
                "Handle": "hd829a22eg85d7g4d65gba69g2cbcd07dbab6",
                "Value": "Even the splendour of Neverwinter must be tainted by Raphael's influence. "
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
