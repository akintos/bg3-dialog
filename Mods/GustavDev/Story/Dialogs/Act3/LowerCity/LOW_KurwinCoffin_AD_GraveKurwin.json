          "TagTexts": [
            {
              "LineId": "5b9a6bcc-2a4e-49b0-974c-655fae2574c8",
              "Text": {
                "Handle": "hefbf3601g9b0dg4170gb6a1gc519e18e032b",
                "Value": "'Kurwin', from the courier's note. There was some matter with Nine-Fingers, if I recall."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
