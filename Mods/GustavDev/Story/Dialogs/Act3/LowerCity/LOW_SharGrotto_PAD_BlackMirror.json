      "SetFlags": [],
      "TaggedTextList": [
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "ffd08582-7396-4cac-bcd4-8f9cd0fd8ef3"
                ],
                "TagNames": [
                  "REALLY_ASTARION"
                ]
              }
            ],
            "RuleString": "REALLY_ASTARION"
          },
          "TagTexts": [
            {
              "LineId": "cc886669-2ccc-4f09-8419-5073b9eee3ef",
              "Text": {
                "Handle": "h524e50e9g4f7cg4d02g98cbg45bc20c8f68f",
                "Value": "A mirror that reflects nothing is bad enough, but this one feels hungry..."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "b5682d1d-c395-489c-9675-1f9b0c328ea5"
                ],
                "TagNames": [
                  "REALLY_LAEZEL"
                ]
              }
            ],
            "RuleString": "REALLY_LAEZEL"
          },
          "TagTexts": [
            {
              "LineId": "e84ed83e-110d-48a2-8578-8e6df052114b",
              "Text": {
                "Handle": "h8b2fb676g9574g4084gaae2g2eecd5b628e7",
                "Value": "The mirror reflects nothing - it only consumes.<br>"
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "9b0354c0-56d9-4723-8034-918ac9abab19"
                ],
                "TagNames": [
                  "REALLY_GALE"
                ]
              }
            ],
            "RuleString": "REALLY_GALE"
          },
          "TagTexts": [
            {
              "LineId": "06892e7f-3f06-4ae8-8721-0b81431dd027",
              "Text": {
                "Handle": "h3c3bd141g57c8g411bga591g3800be6c8b01",
                "Value": "No ordinary mirror. It does not reflect, but consumes. How many have fallen prey to its darkness?"
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "5f40def5-d3ec-4698-a367-01a339888956"
                ],
                "TagNames": [
                  "REALLY_WYLL"
                ]
              }
            ],
            "RuleString": "REALLY_WYLL"
          },
          "TagTexts": [
            {
              "LineId": "e38c3297-9311-4ab2-b00b-870a8b0b5a5a",
              "Text": {
                "Handle": "h67cf34d2ga9fag42cdgbe44g303f35255f5e",
                "Value": "Nothing but blackness, deep as the Shadowfell. Talk about staring into the abyss...<br>"
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "1a2f70d6-8ead-4eb5-a824-79ee1971764a"
                ],
                "TagNames": [
                  "REALLY_KARLACH"
                ]
              }
            ],
            "RuleString": "REALLY_KARLACH"
          },
          "TagTexts": [
            {
              "LineId": "00978587-f9ad-405a-82d7-4d573dc9120b",
              "Text": {
                "Handle": "h2e587e4dg0dd3g47bfg8800g7b8c08aecbb4",
                "Value": "No reflection. Just darkness."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "8457eb5f-036c-4143-b6cf-447a9f555c7a"
                ],
                "TagNames": [
                  "REALLY_JAHEIRA"
                ]
              }
            ],
            "RuleString": "REALLY_JAHEIRA"
          },
          "TagTexts": [
            {
              "LineId": "7dac1a4f-f437-4216-89ed-fcca3ba956cc",
              "Text": {
                "Handle": "h8a98a0bagab32g41bfg9ff7g327e53df4180",
                "Value": "A mirror, reflecting nothing. Perfect for the Sharran who dare not look upon their own face."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "9b8709f9-8d2a-4b4e-a465-8505c561d7f5"
                ],
                "TagNames": [
                  "REALLY_HALSIN"
                ]
              }
            ],
            "RuleString": "REALLY_HALSIN"
          },
          "TagTexts": [
            {
              "LineId": "8fb46ced-154c-4a27-bc0e-886d92ea4af2",
              "Text": {
                "Handle": "h7992f102g2c2fg469agab07gb2591d01126b",
                "Value": "No light escapes that mirror. No life reflects back in it. It reminds me of the Shadowfell..."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "3e84e1cd-2193-4f9f-80b4-c2ededefaea6"
                ],
                "TagNames": [
                  "REALLY_MINTHARA"
                ]
              }
            ],
            "RuleString": "REALLY_MINTHARA"
          },
          "TagTexts": [
            {
              "LineId": "fffcebd2-4e40-4f04-a2e1-df5665aa6c57",
              "Text": {
                "Handle": "h63de7c6bg865dg4ed1g9d70g58c49bf5baa8",
                "Value": "Looking into the mirror, I feel myself erased from the world."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "aeb694fc-83fb-452d-819a-b97ba442dc42"
                ],
                "TagNames": [
                  "REALLY_MINSC"
                ]
              }
            ],
            "RuleString": "REALLY_MINSC"
          },
          "TagTexts": [
            {
              "LineId": "55a5dcca-54b2-4191-a0d0-bf2ed9babead",
              "Text": {
                "Handle": "h154043d0g14cag4c4dga67fg5f92bfd3fb0e",
                "Value": "What use a mirror, if my hamster cannot see his handsome face? Here, Boo - use my shiny scalp."
              }
            }
          ]
        },
        {
          "HasTagRule": false,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [],
            "RuleString": ""
          },
          "TagTexts": [
            {
              "LineId": "7acbc2c9-0c75-4944-8f6d-d021b48bfbac",
              "Text": {
                "Handle": "h1963270cg9037g45begac08g79600ff49d27",
                "Value": "That mirror reflects nothing - it only consumes, and offers darkness."
              }
            }
          ]
        }
      ],
