      "TaggedTextList": [
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "ffd08582-7396-4cac-bcd4-8f9cd0fd8ef3"
                ],
                "TagNames": [
                  "REALLY_ASTARION"
                ]
              }
            ],
            "RuleString": "REALLY_ASTARION"
          },
          "TagTexts": [
            {
              "LineId": "e45d3032-2ce5-4e82-b937-84d3239e6651",
              "Text": {
                "Handle": "hfb47c91cg77c3g4ff0gb133gb7ae436ac14a",
                "Value": "Off we go, then."
              }
            },
            {
              "LineId": "16d4a0a8-052c-4dc2-8055-1a5300886c85",
              "Text": {
                "Handle": "h4fefbfe8gd5e4g44f8g8621g537f54a1b6f4",
                "Value": "Well, some rest's better than none."
              }
            },
            {
              "LineId": "b44555f1-eb54-41a4-b7b2-eb0b2ab3104c",
              "Text": {
                "Handle": "hb18f6d75g3722g46fegacdag71f56dc2d664",
                "Value": "Enough sitting around - let's go hurt someone."
              }
            },
            {
              "LineId": "b8c2aa11-57ec-4fcb-baa5-419ea313b146",
              "Text": {
                "Handle": "hd3bd6ecfgc362g4a48ga172gdc7310e30d5d",
                "Value": "All right, time to see what new horror's waiting for us."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "b5682d1d-c395-489c-9675-1f9b0c328ea5"
                ],
                "TagNames": [
                  "REALLY_LAEZEL"
                ]
              }
            ],
            "RuleString": "REALLY_LAEZEL"
          },
          "TagTexts": [
            {
              "LineId": "82de28b7-dd44-4a72-b517-2b5071d36753",
              "Text": {
                "Handle": "h9b4d7ddeg51c4g4546g9ef8g9a2d2f485a94",
                "Value": "Rested enough. Now - forward."
              }
            },
            {
              "LineId": "b81448f4-0c11-4b0f-9d9f-fec1fb9d53da",
              "Text": {
                "Handle": "h2b498a92g2c48g4014g96aagf2f0e972784e",
                "Value": "That should more than suffice."
              }
            },
            {
              "LineId": "117cee0e-1a26-4dc8-91b8-30e4096b999a",
              "Text": {
                "Handle": "h3033815ege095g41d4ga17eg8fa6a5532d36",
                "Value": "Enough time wasted. Battle awaits."
              }
            },
            {
              "LineId": "1f8e115f-5714-4155-bd78-bc52ffbfd79b",
              "Text": {
                "Handle": "heaca36c8ga2b2g4915gb113g5a9093b63c1c",
                "Value": "More than prepared."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "9b0354c0-56d9-4723-8034-918ac9abab19"
                ],
                "TagNames": [
                  "REALLY_GALE"
                ]
              }
            ],
            "RuleString": "REALLY_GALE"
          },
          "TagTexts": [
            {
              "LineId": "f2fdbdba-6751-4384-8208-646189270842",
              "Text": {
                "Handle": "h649f6b1cg367fg4fb7gbc93g2091b384423e",
                "Value": "That hit the spot."
              }
            },
            {
              "LineId": "e399557f-0da7-4d63-8446-399a1c496cca",
              "Text": {
                "Handle": "hf31ff77bg54bcg4c18g85a7gc6b1b243cacb",
                "Value": "A little respite does wonders."
              }
            },
            {
              "LineId": "4806bc0f-f135-4522-bb99-5729d4300ed8",
              "Text": {
                "Handle": "h64141bf9g0a5cg4effga269g8704583b3bfe",
                "Value": "Patched up and pushing on."
              }
            },
            {
              "LineId": "60fdc503-6db1-4aaf-960f-b068cb771bc2",
              "Text": {
                "Handle": "h913cd2f9gca36g4496g8c55g1943f73a9fc6",
                "Value": "Lovely little pick me up."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "642d2aee-e3df-47e3-9f47-bbcd441bb9e0"
                ],
                "TagNames": [
                  "REALLY_SHADOWHEART"
                ]
              }
            ],
            "RuleString": "REALLY_SHADOWHEART"
          },
          "TagTexts": [
            {
              "LineId": "33db1f8e-1f39-468b-b37c-8d75b86244cf",
              "Text": {
                "Handle": "h728fbe83gaf89g4ca4g89d1gd6f4622c71b5",
                "Value": "I can keep going a little longer, now."
              }
            },
            {
              "LineId": "159c5936-3268-4471-a948-c15bf42ede89",
              "Text": {
                "Handle": "h35c0b663g6558g4467gb0c4g30a3d0b9ae60",
                "Value": "That helped."
              }
            },
            {
              "LineId": "02036704-177c-4cd7-92c2-8b5a9b74b8d8",
              "Text": {
                "Handle": "h86d65913ge249g4e35gbc98ge226c2224187",
                "Value": "A welcome rest."
              }
            },
            {
              "LineId": "86c25899-2943-45b7-b042-a7c07a91c085",
              "Text": {
                "Handle": "h48a7ee1dg3e50g4314gb1b5g4a9e113418f1",
                "Value": "Just a brief respite."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "5f40def5-d3ec-4698-a367-01a339888956"
                ],
                "TagNames": [
                  "REALLY_WYLL"
                ]
              }
            ],
            "RuleString": "REALLY_WYLL"
          },
          "TagTexts": [
            {
              "LineId": "2d2165da-d0e7-41a4-aabc-4e63ee40264c",
              "Text": {
                "Handle": "h93597b77g42a2g43bag82ffg9e6e0f113f78",
                "Value": "That'll have to do."
              }
            },
            {
              "LineId": "446f2ae4-c52a-4484-9b74-7ab2a5c1935a",
              "Text": {
                "Handle": "h0c095dfegb6f6g45c5g8ec2g3463f91cdb29",
                "Value": "Freshened up and ready to fight."
              }
            },
            {
              "LineId": "10b046a4-94a6-451f-935d-a70fb6df85c1",
              "Text": {
                "Handle": "h12c819cdgbcabg4433ga928g7c13c5aaf8c1",
                "Value": "Aaaaaand here I go."
              }
            },
            {
              "LineId": "150ceef3-1dd9-4a0a-b496-688230109146",
              "Text": {
                "Handle": "h45c4614fg2c9fg4172gaf89g0c99baee95cd",
                "Value": "Awake and alert."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "1a2f70d6-8ead-4eb5-a824-79ee1971764a"
                ],
                "TagNames": [
                  "REALLY_KARLACH"
                ]
              }
            ],
            "RuleString": "REALLY_KARLACH"
          },
          "TagTexts": [
            {
              "LineId": "d8e3a1d3-9697-453e-b647-66671772090b",
              "Text": {
                "Handle": "hfe48fd16g1e2cg4d74ga931g743b8b42e7c3",
                "Value": "Nice little nap."
              }
            },
            {
              "LineId": "fee311c9-2108-4fe7-8dcc-40b0d9a267cb",
              "Text": {
                "Handle": "h3fd4bbcag483fg4bd4g997cg0b2065fb7ac6",
                "Value": "Feeling fresh."
              }
            },
            {
              "LineId": "1e797453-606a-4d1b-bbc3-d4e4226cac0e",
              "Text": {
                "Handle": "h1126e705gd815g4302g9cf6g44383047e851",
                "Value": "Ah, that put the wind back in my sails."
              }
            },
            {
              "LineId": "524eddf8-cb7e-4488-9b8a-b7970166630b",
              "Text": {
                "Handle": "h9b7b6cedg6b8dg4aeeg83c3g3a5e6603c113",
                "Value": "*Yawn!*"
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "8457eb5f-036c-4143-b6cf-447a9f555c7a"
                ],
                "TagNames": [
                  "REALLY_JAHEIRA"
                ]
              }
            ],
            "RuleString": "REALLY_JAHEIRA"
          },
          "TagTexts": [
            {
              "LineId": "cdae7098-bb2f-4090-bee1-4f07989c9fab",
              "Text": {
                "Handle": "h464d3107gc417g4050gac5eg8a5cb63a59cf",
                "Value": "Enough to keep us going, for now."
              }
            },
            {
              "LineId": "1e0ff996-d452-4cc6-a4f7-8e9722da1bd5",
              "Text": {
                "Handle": "h40115c24g543ag494cg8336g23bdf32607e2",
                "Value": "*Grunt.*"
              }
            },
            {
              "LineId": "77069c50-9b90-4716-b945-a29166e61ef5",
              "Text": {
                "Handle": "hc35b620agcf0dg4f66g94a7g14f6aefa4042",
                "Value": "Agh. Is it the earth beneath that crunches, or the bones within?"
              }
            },
            {
              "LineId": "efb1b5d5-700d-426a-a183-0a6e7487a69a",
              "Text": {
                "Handle": "hc2f5cee9g6aa6g4142gad72g68aeb1907f30",
                "Value": "Come, laggards. There is still enough to be done this day."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "9b8709f9-8d2a-4b4e-a465-8505c561d7f5"
                ],
                "TagNames": [
                  "REALLY_HALSIN"
                ]
              }
            ],
            "RuleString": "REALLY_HALSIN"
          },
          "TagTexts": [
            {
              "LineId": "e4f93b22-eb73-4b90-bacf-195bcb4a8d24",
              "Text": {
                "Handle": "h2c28f468g1abeg479bg95e5gdac7275e4672",
                "Value": "A welcome rest."
              }
            },
            {
              "LineId": "eb679a27-0015-49e3-898f-aa0de039e613",
              "Text": {
                "Handle": "he14bb8c2gfbcbg49bbgab53ge693036806a6",
                "Value": "Most refreshing."
              }
            },
            {
              "LineId": "0f0b6592-c667-46b6-827e-edc993aeb3cb",
              "Text": {
                "Handle": "hbdd2136bg413eg4c5egbdceg1216417cf56b",
                "Value": "I needed that."
              }
            },
            {
              "LineId": "22a5b06b-d9b4-4549-b85f-98de81301b52",
              "Text": {
                "Handle": "hfaecff4eg8810g45eega5d9g9d07b1f1567f",
                "Value": "A short respite, but better than nothing."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "3e84e1cd-2193-4f9f-80b4-c2ededefaea6"
                ],
                "TagNames": [
                  "REALLY_MINTHARA"
                ]
              }
            ],
            "RuleString": "REALLY_MINTHARA"
          },
          "TagTexts": [
            {
              "LineId": "922b23ed-9b57-48b3-b123-b8f527643c22",
              "Text": {
                "Handle": "h4709f89ag9195g4b7dgb24bgd8e2f823aae8",
                "Value": "I hate to rest while our enemies still draw breath."
              }
            },
            {
              "LineId": "21b6fb4b-4924-4965-8ced-cd9f154eafaa",
              "Text": {
                "Handle": "h12938146g7030g448ag9ce9g10fdd59f7692",
                "Value": "Come - we are rested enough."
              }
            },
            {
              "LineId": "8c634d89-9562-4813-b675-3501dbeccaa1",
              "Text": {
                "Handle": "h3f27c814gcfe3g4e0bg8b56ga4c26d59019d",
                "Value": "Onward."
              }
            },
            {
              "LineId": "969652c9-8370-4534-8f1d-6cb98052123c",
              "Text": {
                "Handle": "hb3aea17cg5b8fg44d7g8e98gea168fcbc584",
                "Value": "No more rest for the wicked."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "aeb694fc-83fb-452d-819a-b97ba442dc42"
                ],
                "TagNames": [
                  "REALLY_MINSC"
                ]
              }
            ],
            "RuleString": "REALLY_MINSC"
          },
          "TagTexts": [
            {
              "LineId": "a57106ed-b023-4dc4-a677-4d9bc91a0980",
              "Text": {
                "Handle": "he70c2a13g8a06g4d2aga4e8g2f9900788d65",
                "Value": "Watch out, evil. None nap so powerfully as Boo!"
              }
            },
            {
              "LineId": "a778b2fc-9bad-480b-aefa-7f8ed45fbea8",
              "Text": {
                "Handle": "hb15a0746ga70cg4171g8fd3gb8e39087430d",
                "Value": "My hamster, he is so full of beans!"
              }
            },
            {
              "LineId": "b033899a-b519-43a0-9d9c-7e7068d2ba54",
              "Text": {
                "Handle": "ha19c73c0g757cg438cg8c48g20d244680a3b",
                "Value": "I did not sleep. I did not snore. It was a battle-trance."
              }
            },
            {
              "LineId": "091bbfa2-81c2-4d0a-a4b0-dcca1e921d85",
              "Text": {
                "Handle": "h9d471c47g5a87g449ega452gf417ba9b9ab9",
                "Value": "Ah, but it was good to feel the breeze between my toes. Back to the road."
              }
            }
          ]
        },
        {
          "HasTagRule": false,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [],
            "RuleString": ""
          },
          "TagTexts": [
            {
              "LineId": "3af92df1-5d19-4310-b98d-61c46a314d51",
              "Text": {
                "Handle": "h03ff47dcge157g4521gb057g2425f5d83b18",
                "Value": "Better than nothing."
              }
            },
            {
              "LineId": "6a4472a3-ea24-4bb1-8c2b-49c0a1908522",
              "Text": {
                "Handle": "h9f5d77f5gd639g46a4gb42fg8b980309fcd1",
                "Value": "That took the edge off."
              }
            },
            {
              "LineId": "7198f07d-a6d0-48b5-8ee5-fb921ddd3c25",
              "Text": {
                "Handle": "h608ea269gca3bg457ag9053g67a21a9422b8",
                "Value": "I'll feel that one later."
              }
            },
            {
              "LineId": "f3b79a6d-1714-41b1-a89c-6bdd0c0b75c2",
              "Text": {
                "Handle": "h89eb7ccbgcdbeg45fcga07fgdb70c13a20f8",
                "Value": "Time to get going."
              }
            }
          ]
        }
