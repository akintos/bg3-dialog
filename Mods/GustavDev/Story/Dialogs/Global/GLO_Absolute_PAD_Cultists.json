          "TagTexts": [
            {
              "LineId": "02eaba3a-7327-4ef1-850e-5a0f89de781f",
              "Text": {
                "Handle": "h6780e887g8006g48e4g8b34g7ac5ad7fca14",
                "Value": "Wait, True Souls are infected like us? Why do they think they're talking to a god?"
              }
            }
          ]
        }
