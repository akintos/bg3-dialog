          "TagTexts": [
            {
              "LineId": "e5f525f0-2812-491a-9e7a-7a18218941c3",
              "Text": {
                "Handle": "hb3c8b8bag5799g4024g987dg0121520ca152",
                "Value": "The True Soul was infected with a tadpole. This Absolute of theirs must be a <i>ghaik</i> hallucination."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "1a2f70d6-8ead-4eb5-a824-79ee1971764a"
                ],
                "TagNames": [
                  "REALLY_KARLACH"
                ]
              }
            ],
            "RuleString": "REALLY_KARLACH"
          },
          "TagTexts": [
            {
              "LineId": "111418bb-f90f-4817-880d-b4685547b987",
              "Text": {
                "Handle": "h54ae9c6bg0775g40b7g857dg1022e8bf6fb8",
                "Value": "These True Souls think they're talking to a god. But it's just the tadpole, right?"
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "9b0354c0-56d9-4723-8034-918ac9abab19"
                ],
                "TagNames": [
                  "REALLY_GALE"
                ]
              }
            ],
            "RuleString": "REALLY_GALE"
          },
          "TagTexts": [
            {
              "LineId": "f260ea1b-7049-41b5-ba30-8f19d896109b",
              "Text": {
                "Handle": "h2847e16dg405dg4fe2g9373gb19a216e916a",
                "Value": "They can hear the tadpoles speak to them, and they think it's a new god."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "264a6880-9a51-429c-a9fc-97f8952baf90"
                ],
                "TagNames": [
                  "REALLY_GENERIC"
                ]
              }
            ],
            "RuleString": "REALLY_GENERIC"
