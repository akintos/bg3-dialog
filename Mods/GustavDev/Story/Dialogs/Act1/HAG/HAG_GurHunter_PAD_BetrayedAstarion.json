          "TagTexts": [
            {
              "LineId": "0b519e51-c0ae-4f4a-9cf6-3e377c3e16f8",
              "Text": {
                "Handle": "h8dd0bf10gad6eg4477ga861gc5b83b2042fa",
                "Value": "Astarion's as good as dead. I won't waste another thought on him."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
