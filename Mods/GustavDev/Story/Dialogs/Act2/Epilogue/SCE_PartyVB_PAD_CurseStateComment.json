      "SetFlags": [],
      "TaggedTextList": [
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
