          "TagTexts": [
            {
              "LineId": "7acdc29b-1372-4662-b172-9a4964aff043",
              "Text": {
                "Handle": "h1efcb44bg79ffg41a8g86f4gfdf8b5ba5977",
                "Value": "Quiet but uneasy. Even in the Moonmaiden's refuge, the curse feels close at hand."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "9b0354c0-56d9-4723-8034-918ac9abab19"
                ],
                "TagNames": [
                  "REALLY_GALE"
                ]
              }
            ],
            "RuleString": "REALLY_GALE"
          },
          "TagTexts": [
            {
              "LineId": "ed60a729-0bbe-43e4-8241-9af2bc7b96b4",
              "Text": {
                "Handle": "he1a5f317gae21g4fc2g8c61gee5b93a7f4c3",
                "Value": "Far better to encourage such glimmers of light amongst the darkness.  Who knows where they may lead?"
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "5f40def5-d3ec-4698-a367-01a339888956"
                ],
                "TagNames": [
                  "REALLY_WYLL"
                ]
              }
            ],
            "RuleString": "REALLY_WYLL"
          },
          "TagTexts": [
            {
              "LineId": "dceb2f69-d340-4465-87be-adc0cfec943f",
              "Text": {
                "Handle": "h038da301g5ec8g45a9g9ee4g7b07d426bff9",
                "Value": "In a land of darkness, Selûne's faithful learn make their own light."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "1a2f70d6-8ead-4eb5-a824-79ee1971764a"
                ],
                "TagNames": [
                  "REALLY_KARLACH"
                ]
              }
            ],
            "RuleString": "REALLY_KARLACH"
          },
          "TagTexts": [
            {
              "LineId": "c848ec0a-88cd-4b51-85b3-ee92d25d41a3",
              "Text": {
                "Handle": "hd475ff5fg0678g4957ga77dg7920fcd4ef09",
                "Value": "I liked you before, but I extra like you now."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "9b8709f9-8d2a-4b4e-a465-8505c561d7f5"
                ],
                "TagNames": [
                  "REALLY_HALSIN"
                ]
              }
