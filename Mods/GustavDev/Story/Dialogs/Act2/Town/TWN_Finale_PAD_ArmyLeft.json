          },
          "TagTexts": [
            {
              "LineId": "eb1a56fa-be25-49d8-a58e-58d575cc369f",
              "Text": {
                "Handle": "h0a11d59dgb29bg4a89gb342g04852c1593cd",
                "Value": "The Absolute's army trampled through here. We can't let them get to Baldur's Gate."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
