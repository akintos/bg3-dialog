          "TagTexts": [
            {
              "LineId": "039a73c4-4c8b-4f8e-9bdc-538b37b2fa22",
              "Text": {
                "Handle": "hbbb7b66cg2788g472ag9ea0gd109a63e8f29",
                "Value": "The Absolute's forces have marched forth. Baldur's Gate is in danger."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
