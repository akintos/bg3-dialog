            "RuleString": "REALLY_WYLL"
          },
          "TagTexts": [
            {
              "LineId": "4e25c188-f757-4814-934f-bb38a4509bbf",
              "Text": {
                "Handle": "h485d41b6gce50g42d8gb94cgdc1052845cd4",
                "Value": "Dark magic - I can feel it. It wasn't used just for reflection, but for <i>collection.</i>"
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "1a2f70d6-8ead-4eb5-a824-79ee1971764a"
                ],
                "TagNames": [
                  "REALLY_KARLACH"
                ]
              }
            ],
            "RuleString": "REALLY_KARLACH"
          },
          "TagTexts": [
            {
              "LineId": "baeae085-39ed-486c-9551-816043ea08de",
              "Text": {
                "Handle": "he96e6454g919ag4d75gb70dg138f8bd6598a",
                "Value": "Used to be magic in this mirror. Something... dark. Shattered, now."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "9b8709f9-8d2a-4b4e-a465-8505c561d7f5"
                ],
                "TagNames": [
                  "REALLY_HALSIN"
                ]
