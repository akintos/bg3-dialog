            ],
            "RuleString": "REALLY_HALSIN"
          },
          "TagTexts": [
            {
              "LineId": "9281d136-c6bf-427a-a3ac-0415d00ea801",
              "Text": {
                "Handle": "hc5775488g0313g403dg8089geb5c6b9687d6",
                "Value": "Sharran magic once imbued this mirror - I can feel it. But it works no more.<br>"
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "3e84e1cd-2193-4f9f-80b4-c2ededefaea6"
                ],
                "TagNames": [
                  "REALLY_MINTHARA"
                ]
