            "RuleString": "REALLY_LAEZEL"
          },
          "TagTexts": [
            {
              "LineId": "092ab035-3fc6-4cb4-ba32-59f5eb353799",
              "Text": {
                "Handle": "hc1040c18g197eg4364gb2cbgb3e4e8f4ee31",
                "Value": "This mirror was an instrument of Shar's faithful. Dark magic still flows from the shards. "
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
