          "TagTexts": [
            {
              "LineId": "6f7d43dc-dfcd-49a9-ab0f-eb26453774f8",
              "Text": {
                "Handle": "h614d0905ga47bg43a2gb104gd7fe6899ea27",
                "Value": "Even in the Underdark, I never saw gloom like this. It's unsettling."
              }
            }
          ]
        },
        {
          "HasTagRule": false,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [],
            "RuleString": ""
          },
          "TagTexts": [
            {
              "LineId": "8f6a33dd-9720-4e22-9d19-3d8b7690b011",
              "Text": {
                "Handle": "h34b84e64g95a7g4bedga985gea1ae4130bb8",
                "Value": "I've never seen darkness like this before. It's unsettling."
              }
