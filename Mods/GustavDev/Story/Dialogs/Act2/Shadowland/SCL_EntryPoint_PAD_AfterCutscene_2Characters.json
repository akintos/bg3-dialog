            "RuleString": "REALLY_LAEZEL"
          },
          "TagTexts": [
            {
              "LineId": "5326e66c-6bc9-46b0-92cd-38ae04cfc81b",
              "Text": {
                "Handle": "h19532f09g20c3g4300g9d60g310ed3f178f1",
                "Value": "I know the dark of wildspace. But I've never known a dark like this."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
