            "RuleString": "REALLY_LAEZEL"
          },
          "TagTexts": [
            {
              "LineId": "e5a0f049-ebb8-4318-8734-168008fd04df",
              "Text": {
                "Handle": "h793d6cf7gdfb7g4366g8437gac889ebff1df",
                "Value": "There was a great battle here. The ground stained red with blood long dried."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
