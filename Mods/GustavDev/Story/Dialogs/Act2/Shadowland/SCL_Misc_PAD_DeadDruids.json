            "RuleString": "REALLY_JAHEIRA"
          },
          "TagTexts": [
            {
              "LineId": "25e0fbd6-d1a0-4d88-a235-a861525ce5a7",
              "Text": {
                "Handle": "h28a269fdge1c1g4eaaga315g94a4253507e8",
                "Value": "Forces from the Emerald Grove. Many stood against Ketheric - only we lucky few survived him."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "9b8709f9-8d2a-4b4e-a465-8505c561d7f5"
                ],
                "TagNames": [
                  "REALLY_HALSIN"
                ]
              }
            ],
            "RuleString": "REALLY_HALSIN"
          },
          "TagTexts": [
            {
