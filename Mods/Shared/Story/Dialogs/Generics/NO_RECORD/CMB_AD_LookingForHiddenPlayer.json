          "TagTexts": [
            {
              "LineId": "178d8e71-bbaf-469a-bcb0-37c88f0982f8",
              "Text": {
                "Handle": "h808e7881g3464g40a6gbbfbg04f11ee8dd8a",
                "Value": "Show yourself!"
              }
            },
            {
              "LineId": "b7e106c4-d74b-4875-952e-e80593ed1347",
              "Text": {
                "Handle": "h4c99bee1ge16fg4412g9081g9ca4f94ae913",
                "Value": "You can't hide forever."
              }
            },
            {
              "LineId": "1a6f4279-f584-49b6-86ad-c47bd7f3f3c2",
              "Text": {
                "Handle": "hcdf27879g2dfag4b48g84eeg2b9ae86747a4",
                "Value": "Let's get this over with!"
              }
            },
            {
              "LineId": "f1d823b4-44ca-44aa-888d-9408e9633657",
              "Text": {
                "Handle": "hf8d71a37g8e31g4832g9dcagd0c7da1c989b",
                "Value": "Too scared to face me?"
              }
            },
            {
              "LineId": "3f3eb5b8-4930-4658-8e7f-43f45f375e86",
              "Text": {
                "Handle": "heb65fdeeg043cg456ag8e52g2ded36b33665",
                "Value": "You're better at hiding than fighting, coward."
              }
            },
            {
              "LineId": "85c12c7b-d550-43d1-9781-fd27dc73cc27",
              "Text": {
                "Handle": "h9fd0ba39g27dbg4f69g93f2g94d90277f769",
                "Value": "This isn't over - not until you're dead."
              }
            },
            {
              "LineId": "0306f54b-f97b-4d37-a3a3-302653666737",
              "Text": {
                "Handle": "hbd60d59ege543g46a7ga1e6g24f7b2975371",
                "Value": "Stop hiding and I'll make it quick."
              }
            },
            {
              "LineId": "9e31264c-24ca-4162-8d8d-20c20f6a17c0",
              "Text": {
                "Handle": "h9298122eg2d6fg41ecg9f93g42481cb6fba8",
                "Value": "You're only making it worse for yourself!"
              }
            }
          ]
        }
