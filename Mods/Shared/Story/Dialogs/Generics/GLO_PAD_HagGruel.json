          "TagTexts": [
            {
              "LineId": "87a55389-71d6-3e4b-44b4-7b56596200d3",
              "Text": {
                "Handle": "h1ae5ec90g987eg43a0gac9cgeaae4ef03159",
                "Value": "Well that's my appetite gone for a while."
              }
            },
            {
              "LineId": "1d53810f-eb26-4c0f-2ce9-9b52c1a99766",
              "Text": {
                "Handle": "h5b6941ceg0bfag434aga496g76c58b5f58d0",
                "Value": "Ugh. Looks like it's already been digested once or twice."
              }
            },
