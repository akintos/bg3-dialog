          "TagTexts": [
            {
              "LineId": "6c1929ad-864f-a76b-b924-8f9aaabe3df5",
              "Text": {
                "Handle": "h58574d74geed3g4a2fgbe87g31fd3a6bb471",
                "Value": "I cannot tell, Boo - is it stew gone bad, or the result of someone <i>eating</i> stew gone bad?"
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
