          },
          "TagTexts": [
            {
              "LineId": "a78483a6-b201-9e3f-22b8-65b1bc42fa92",
              "Text": {
                "Handle": "h16927703g4f9dg4381ga8b9g54d6e993b626",
                "Value": "Good gods, that's disgusting."
              }
            },
            {
              "LineId": "b6cfe8f9-f259-6c2e-993c-f5be96fa26a1",
              "Text": {
                "Handle": "h644c7f5cgd632g4649gbea3ga427232e2fe1",
                "Value": "Smells like a bugbear's morning breath. Best give this dish a wide berth."
              }
            },
            {
              "LineId": "a056b097-3b97-f89b-f167-7ad6911de379",
              "Text": {
                "Handle": "hf5d7e7d9gc829g4bfcga612g05e71dfbbcc5",
                "Value": "Not for me, thanks. I've seen more appetising fare strewn across the floor of a goblin abattoir."
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
                "TagCombineOp": 0,
                "Tags": [
                  "1a2f70d6-8ead-4eb5-a824-79ee1971764a"
                ],
                "TagNames": [
                  "REALLY_KARLACH"
                ]
              }
            ],
            "RuleString": "REALLY_KARLACH"
          },
          "TagTexts": [
            {
              "LineId": "2fffdac0-21a3-a47b-aaef-da1191787dd1",
              "Text": {
                "Handle": "h016ae0a7g9fd2g42f7gb45ag50f7024b1a0c",
                "Value": "Huh. Kinda smells good."
              }
            },
            {
              "LineId": "1369dd01-51fd-4bb1-81a9-2a5c410dac89",
              "Text": {
                "Handle": "h913ad697g2f27g45f5ga979g092da79e73ee",
                "Value": "I'd try it."
              }
            },
            {
              "LineId": "7fd62d3b-364a-4d35-873a-6763c8dae327",
              "Text": {
                "Handle": "h96611470gdf46g462ega383g754e3572df07",
                "Value": "Frogsbreath, maybe?"
              }
            }
          ]
        },
        {
          "HasTagRule": true,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [
              {
                "HasChildRules": false,
