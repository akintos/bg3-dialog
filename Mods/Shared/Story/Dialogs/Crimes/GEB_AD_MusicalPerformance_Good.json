      ],
      "SetFlags": [],
      "TaggedTextList": [
        {
          "HasTagRule": false,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [],
            "RuleString": ""
          },
          "TagTexts": [
            {
              "LineId": "9d45e439-f07a-42c1-ad92-125dda97b484",
              "Text": {
                "Handle": "hb9de16fdgc6b6g4ed4g8173g99dc50b36d5d",
                "Value": "Know any Volo tunes? "
              }
            },
            {
              "LineId": "0646c9cb-acb4-4f13-860a-7891595ddde9",
              "Text": {
                "Handle": "h699bec11g90eag4095gbf75g04b641dbdb9d",
                "Value": "Not half bad."
              }
            },
            {
              "LineId": "a7474b94-1fcd-44f5-9486-28f2d1bee780",
              "Text": {
                "Handle": "h4d598eb4gc417g47ebg92e2ge9c17865d40c",
                "Value": "Encore!"
              }
            },
            {
              "LineId": "ce05099a-76e3-43ae-a201-1ed0997bcc8a",
              "Text": {
                "Handle": "hbb26323fgbdc2g4928g9a6fg94104b1d1f66",
                "Value": "Hey, you're pretty good!"
              }
            },
            {
              "LineId": "ada55525-2e1b-4c65-98d8-7e267fc83f4a",
              "Text": {
                "Handle": "h60caf7c5g8ebfg43f4g98dbg14b3ab028287",
                "Value": "I like that."
              }
            },
            {
              "LineId": "d1a0ef2c-33c4-4982-b3bc-2f4bde05deef",
              "Text": {
                "Handle": "h2a844019g037cg4873gbdfcge85f59831e74",
                "Value": "Nice tunes. "
              }
            },
            {
              "LineId": "5f9bd962-238e-4f41-8061-84818f043eae",
              "Text": {
                "Handle": "h96dc236fg903eg43a9gb912g78391006117e",
                "Value": "Well played."
              }
            },
            {
              "LineId": "f972cd4e-409a-4909-b428-164eb16cf821",
              "Text": {
                "Handle": "hb03db576g7d46g433eg8a02gda61e30c353c",
                "Value": "Sort of wish I could play now. "
              }
            }
          ]
        }
      ],
