      "Children": [],
      "CheckFlags": [
        {
          "Flags": [
            {
              "UUID": "ee978587-6c68-4186-9bfc-3b3cc719a835",
              "value": true,
              "paramval": 0,
              "Name": "KID"
            }
          ],
          "Type": "Tag"
        }
      ],
      "SetFlags": [],
      "TaggedTextList": [
        {
          "HasTagRule": false,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [],
            "RuleString": ""
          },
          "TagTexts": [
            {
              "LineId": "415ffedc-dcc1-4652-ba30-b3a8bac80f18",
              "Text": {
                "Handle": "ha74004edg3d61g4504gb664g05feeabc756c",
                "Value": "That sounds nice. "
              }
            },
            {
              "LineId": "e97d0ca0-f21a-489f-a819-94cb03f847d1",
              "Text": {
                "Handle": "h5dbbe369g9666g49a4g8d69g7badaa80c30e",
                "Value": "You sing better than mum. She's rubbish. "
              }
            },
            {
              "LineId": "7598ea48-b96f-45cd-b82a-fa5ade2b8319",
              "Text": {
                "Handle": "h98643874g7d09g49eagbadage97d4f978ddf",
                "Value": "I wanna learn to play!"
              }
            },
            {
              "LineId": "fe07cfc5-4ed7-4eb5-a99c-9a2865f3444a",
              "Text": {
                "Handle": "h50156560g369ag4961gaa18gbdc5688bdefc",
                "Value": "Dad won't let me play the lute. "
              }
            },
            {
              "LineId": "06965205-b878-48ec-9e37-74d023007a81",
              "Text": {
                "Handle": "h7fe7b6bfgbb51g4f89g953dg38d800639c3a",
                "Value": "How'd you learn to play so good?"
              }
            },
            {
              "LineId": "571a3485-27c8-4ea3-9357-5fe1affcd637",
              "Text": {
                "Handle": "h9dcc020dg27dag41c2g961fg2ea370c8fb43",
                "Value": "I want to play when I grow up!"
              }
            },
            {
              "LineId": "f9e74ba0-6294-4263-b037-562fa661e8a2",
              "Text": {
                "Handle": "h4980bdecg7507g47cdg8aa1g6fe904f6b9f7",
                "Value": "Can you teach me!"
              }
            },
            {
              "LineId": "2e111a78-717f-47b9-b265-308ae53a0b06",
              "Text": {
                "Handle": "h676a87b0ge2acg4d29g9dc9g888c1a530536",
                "Value": "I wish they taught music in school."
              }
            }
          ]
        }
      ],
