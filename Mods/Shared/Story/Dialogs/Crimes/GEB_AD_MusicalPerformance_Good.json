    "ddf336a1-123e-74ce-7da0-fd5249bc7308": {
      "UUID": "ddf336a1-123e-74ce-7da0-fd5249bc7308",
      "Constructor": "TagGreeting",
      "EndNode": true,
      "Root": true,
      "ShowOnce": false,
      "SpeakerNo": 0,
      "AddressedSpeaker": 0,
      "Children": [],
      "CheckFlags": [],
      "SetFlags": [],
      "TaggedTextList": [
        {
          "HasTagRule": false,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [],
            "RuleString": ""
          },
          "TagTexts": [
            {
              "LineId": "bebc0120-f77e-4e97-9ba0-d9b1c683a72a",
              "Text": {
                "Handle": "h96da78ebgd497g492bg9027gb6e0d294ecbd",
                "Value": "Beautiful playing. "
              }
            },
            {
              "LineId": "be276528-dd40-4b5d-b34c-8c3845f55520",
              "Text": {
                "Handle": "h9f567c0bg1259g40cbgab69gcce1312ce3ad",
                "Value": "I've heard <i>much</i> worse."
              }
            },
            {
              "LineId": "8d3a2014-f015-46f0-ae69-c4d8ef969e60",
              "Text": {
                "Handle": "ha4da7ad8gb544g4023gb4c3g23d3ba555cbc",
                "Value": "Who taught you? You're great!"
              }
            },
            {
              "LineId": "ebe981d3-d6e9-4347-b49f-93cff553653f",
              "Text": {
                "Handle": "h7c52c364g1b2fg4c34g8651g5a348895add8",
                "Value": "Keep going, you sound lovely. "
              }
            },
            {
              "LineId": "2ff99c55-b60b-429a-ba04-51713c39dd55",
              "Text": {
                "Handle": "h06c91405gba05g46e3gae00g92f166f29773",
                "Value": "Not heard a tune like that in ages."
              }
            },
            {
              "LineId": "0662288f-90b9-42b8-9401-fab80a5104ef",
              "Text": {
                "Handle": "h294da64fg7272g4428g8bbfgee7ed3fa90ef",
                "Value": "Great little ditty."
              }
            },
            {
              "LineId": "16b0726b-c1a5-4c92-b0e9-fd931af13634",
              "Text": {
                "Handle": "hd40c82f9g9eb2g4667g99bbge21a13fb969d",
                "Value": "Do you know <i>'Nobles Have A Certain Spice'?</i>"
              }
            },
            {
              "LineId": "9528d31f-c52f-4077-acc0-64d3331a1480",
              "Text": {
                "Handle": "h033fe93cgc06fg4a7fga7cegbd4dcdec0334",
                "Value": "Wish I'd learned to play."
              }
            },
            {
              "LineId": "b6c1201d-3009-438e-8efe-1a669c8f7d92",
              "Text": {
                "Handle": "hed729664g48e4g46beg82e7gec85ad94def3",
                "Value": "Where'd you learn that?"
              }
            },
            {
              "LineId": "0fc8aece-b5dd-47df-8314-f4574f812c64",
              "Text": {
                "Handle": "h0ab7870agbb1bg430cgbdfeg7fe9a1402023",
                "Value": "Can you play any Volo?"
              }
            },
            {
              "LineId": "bea5328e-dd36-40b0-b248-50c0303b54b9",
              "Text": {
                "Handle": "h0240143ag081fg4267g83c7gcba0ffb48736",
                "Value": "Encore! "
              }
            },
            {
              "LineId": "d4bc8953-a06c-447d-8962-d446ee0ffeb5",
              "Text": {
                "Handle": "h6e50e5e8g6260g4ce4g89cag795c7baa714a",
                "Value": "I'd pay you, but that was <i>priceless</i>. "
              }
            },
            {
              "LineId": "d022c326-9adc-4c9c-84e8-d97acfbcb938",
              "Text": {
                "Handle": "h475a0ab5g39aeg4179gb10bg946ae137329d",
                "Value": "Keep it up!"
              }
            }
          ]
        }
      ],
