          "TagTexts": [
            {
              "LineId": "8c21b1af-60af-b046-46a3-58aa4e1b5dae",
              "Text": {
                "Handle": "h4de59babg782ag4951g924dg8b402598cb3c",
                "Value": "Yer payin' for that! Lads! To me!"
              }
            },
            {
              "LineId": "91e7ca6f-b4bb-a79c-175a-dc8cdedbecaa",
              "Text": {
                "Handle": "h3ec2fda4ga9d8g4d5cg85c0g95f5d28e5dec",
                "Value": "C'mere, lads! Got some fun for ya."
              }
            },
            {
              "LineId": "dc513c5a-a6d5-2a47-d6c2-0cf3ed7b7ee5",
              "Text": {
                "Handle": "hcd6779a6gcf2ag443dgb982gd12dcd7d4e35",
                "Value": "Hey! This one's looking for trouble!"
              }
            },
            {
              "LineId": "48a784ea-edc0-e006-80d5-b5eb1ca0a825",
              "Text": {
                "Handle": "h65b6db26g2dffg4c1fg9a76g11a9ca20d67f",
                "Value": "Here we go again. To me!"
              }
            },
            {
              "LineId": "cbd9ade7-5fc9-8053-bfcc-181536de312d",
              "Text": {
                "Handle": "h23aa259bgc683g4434gaaf2g9002415cf700",
                "Value": "Not so bright, are ya? To arms!"
              }
            }
          ]
        },
        {
          "HasTagRule": false,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [],
            "RuleString": ""
          },
          "TagTexts": [
            {
              "LineId": "e7fcf757-e39a-412c-31f6-db70644d3354",
              "Text": {
                "Handle": "hb924fa57g2f2ag4d91g867fgafb00b569540",
                "Value": "We'll make you pay!"
              }
            },
            {
              "LineId": "0baf3203-d1fc-f330-162d-c8d193b951c6",
              "Text": {
                "Handle": "h633d00ccg8be1g42d5ga97cgd451ba713b87",
                "Value": "You're DONE for. Hear that? To arms!"
              }
            },
            {
              "LineId": "d142a8cd-4627-ff2c-641d-f5f10bbac19f",
              "Text": {
                "Handle": "he5fa577dg5a7ag47c4gac0aga5a520b47e4a",
                "Value": "Looks like I found a troublemaker. To me!"
              }
            },
            {
              "LineId": "73475b13-4b18-f3ba-7447-208f9a735c21",
              "Text": {
                "Handle": "h67cd7ab1gc0d3g49b3gbbf8g2cea494c6734",
                "Value": "THAT was the wrong move. To arms!"
              }
            },
            {
              "LineId": "a55ea73e-ce51-a6d0-f7fa-df2d32436f3c",
              "Text": {
                "Handle": "h78684e2bg2f36g4f52ga962geb706c95a827",
                "Value": "You thought you could get away with that? To me!"
              }
            },
            {
              "LineId": "a33c1506-c504-43a8-af32-da4243c1d064",
              "Text": {
                "Handle": "hd89f1e5agb9f5g4c94g8769g886ae1b1af9d",
                "Value": "Now you've done it! To arms!"
              }
            }
          ]
        }
      ],
