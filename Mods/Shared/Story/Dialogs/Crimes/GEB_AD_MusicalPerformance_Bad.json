    "b0f90c4e-641b-11c4-4331-490357ae9941": {
      "UUID": "b0f90c4e-641b-11c4-4331-490357ae9941",
      "Constructor": "TagGreeting",
      "EndNode": true,
      "Root": true,
      "ShowOnce": false,
      "SpeakerNo": 0,
      "AddressedSpeaker": 0,
      "Children": [],
      "CheckFlags": [],
      "SetFlags": [],
      "TaggedTextList": [
        {
          "HasTagRule": false,
          "RuleGroup": {
            "TagCombineOp": 0,
            "Rules": [],
            "RuleString": ""
          },
          "TagTexts": [
            {
              "LineId": "e247f849-a017-453e-b52f-41934fe86c31",
              "Text": {
                "Handle": "h7429c56ege9efg407agbff7g63bd0e41eab5",
                "Value": "You should practice more. Elsewhere."
              }
            },
            {
              "LineId": "31b0edb8-972d-4bae-b366-c96b6b5625ee",
              "Text": {
                "Handle": "h6edbe875g3945g4fefg8e37g0cf9bd1a7515",
                "Value": "Can you please be quiet?"
              }
            },
            {
              "LineId": "8fc92dc2-8fb0-433f-b8c4-954bd626bd5f",
              "Text": {
                "Handle": "h2f4aaf90gee34g48c3g868bged43c172a409",
                "Value": "Sounds like killing a cat with another, louder cat. "
              }
            },
            {
              "LineId": "324a841e-258b-4e45-a21a-160200e2f01b",
              "Text": {
                "Handle": "hdb9da627g103dg4e99g9946gac491ce5cca0",
                "Value": "Ugh, you're making my head ache. "
              }
            },
            {
              "LineId": "5b54134d-d16b-417e-8803-bde565341276",
              "Text": {
                "Handle": "h21eec4ceg06b9g4b96g80b4g96c7504fe574",
                "Value": "You sound like the runs feels. "
              }
            },
            {
              "LineId": "35257d79-f5c6-4871-ad67-8e78c1102fa3",
              "Text": {
                "Handle": "he0852662gf576g45e8gba5agb9f94aefd16c",
                "Value": "Ever tried a quieter hobby? Miming, maybe?"
              }
            },
            {
              "LineId": "e305d58a-ecf5-47c6-a100-d6e9f0a45289",
              "Text": {
                "Handle": "h825a4ea4geeffg4e3dga22bga0a58b8d18e9",
                "Value": "Great, I really fancied a headache today."
              }
            },
            {
              "LineId": "05f0f2ae-5a9b-466e-b82b-11219550bd60",
              "Text": {
                "Handle": "h3c9d7670g338bg466cg998bg689e8b1376fe",
                "Value": "Get your act together. For all our sakes. "
              }
            },
            {
              "LineId": "d12ad963-884b-4e7a-b1d2-8fd2eab5e0f7",
              "Text": {
                "Handle": "h410b65f6gc345g44efg9c01ge469768da604",
                "Value": "Can I pay you to stop? "
              }
            },
            {
              "LineId": "4e0438dd-dd4f-4e09-9154-0b5ea60e805d",
              "Text": {
                "Handle": "h66479c60g5ebfg44e9g94d7ga1fde44f22fb",
                "Value": "Is this a practical joke? "
              }
            }
          ]
        }
      ],
