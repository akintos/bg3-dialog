    "ecd391ad-7fb3-2f41-8bfb-b70ed24b397b": {
      "UUID": "ecd391ad-7fb3-2f41-8bfb-b70ed24b397b",
      "Constructor": "TagGreeting",
      "EndNode": true,
      "Root": true,
      "ShowOnce": false,
